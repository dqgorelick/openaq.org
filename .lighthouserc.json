{
  "ci": {
      "collect": {
        "staticDistDir": "dist"
      },
      "healthcheck": {
        "fatal": false
      },
      "assert": {
        "preset": "lighthouse:no-pwa",
        "assertions": {
          "categories:performance": ["warn", {"minScore": 0.9}],
          "categories:accessibility": ["warn", {"minScore": 1}]
        }
      }
   }
}
